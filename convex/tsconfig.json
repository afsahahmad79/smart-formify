{
  "compilerOptions": {
      "noEmit": true,

    "target": "ES2018",
    "lib": ["ES2018"],
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node"]
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules", "convex/dist", "convex/**/*.ts"]
}
